[
  {
    "kernelSource": {
      "name": "PixelExperience/EvolutionX-KernelSU",
      "repo": "https://github.com/PixelExperience-Devices/kernel_xiaomi_polaris",
      "branch": "thirteen",
      "device": "polaris",
      "defconfig": "polaris_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://gitlab.com/jjpprrrr/prelude-clang",
        "branch": "master",
        "name": "prelude-clang",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/PixelExperience/prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
        "branch": "thirteen",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "prelude-clang/bin/clang",
      "externalCommand": {
        "CLANG_TRIPLE": "prelude-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-androidkernel-",
        "LINUX_GCC_CROSS_COMPILE_PREBUILTS_BIN": "gcc/bin"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Shuery-Shuai/AnyKernel3",
      "branch": "polaris"
    }
  },
  {
    "kernelSource": {
      "name": "LineageOS-KernelSU",
      "repo": "https://github.com/LineageOS/android_kernel_xiaomi_sdm845",
      "branch": "lineage-20",
      "device": "polaris",
      "defconfig": "sdm845-perf_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_clang_kernel_linux-x86_clang-r416183b",
        "branch": "lineage-20.0",
        "name": "clang",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
        "branch": "lineage-19.1",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "CLANG_TRIPLE": "gcc/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-androidkernel-",
        "LINUX_GCC_CROSS_COMPILE_PREBUILTS_BIN" :"gcc/bin"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Shuery-Shuai/AnyKernel3",
      "branch": "polaris"
    }
  }
]
